{
  "name": "vue2-breeze",
  "version": "0.1.3",
  "description": "是一款基于vue2的可用于项目开发的实用组件库，目前有水印组件，支持按需引入",
  "main": "dist/breeze.umd.js",
  "keywords": [
    "vue",
    "ui",
    "components",
    "watermark"
  ],
  "author": "wangbingwen",
  "files": [
    "dist",
    "components"
  ],
  "scripts": {
    "build:com": "vue-cli-service build --target lib --name breeze ./components/index.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
